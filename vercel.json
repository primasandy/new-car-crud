{
  "version": 2,
  "builds": [
    {
      "src": "frontend-gui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend-gui/dist"
      }
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && source $HOME/.cargo/env && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-gui/dist/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "mysql+pymysql://dbcar_highsandof:0758b475eda21b0cf72c3832fe928b8dbcea59b6@ajs4v.h.filess.io:3305/dbcar_highsandof",
    "MYSQLCLIENT_CFLAGS": "-I/usr/include/mysql",
    "MYSQLCLIENT_LDFLAGS": "-L/usr/lib/mysql"
  }
}
